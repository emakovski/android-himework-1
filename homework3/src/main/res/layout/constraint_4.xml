<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="bottom"
        android:text="Android"
        android:layout_marginStart="20dp"
        android:textColor="@color/black"
        android:textSize="30sp"
        android:textStyle="bold"
        android:layout_marginBottom="20dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/img1"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:background="#a9a9a9"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/textView1"
        app:layout_constraintLeft_toLeftOf="@id/textView1"/>

    <TextView
        android:id="@+id/tx125"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="125"
        android:gravity="bottom"
        android:layout_marginStart="30dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@id/img1"
        app:layout_constraintTop_toTopOf="@id/img1"/>
    <TextView
        android:id="@+id/posts"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:text="Posts"
        android:gravity="bottom"
        android:layout_marginStart="25dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@id/img1"
        app:layout_constraintTop_toTopOf="@id/img1"/>

    <TextView
        android:id="@+id/tx20k"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="20K"
        android:gravity="bottom"
        android:layout_marginStart="50dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@id/tx125"
        app:layout_constraintTop_toTopOf="@id/img1"/>
    <TextView
        android:id="@+id/flw"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:text="Followers"
        android:gravity="bottom"
        android:layout_marginStart="15dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@id/posts"
        app:layout_constraintTop_toTopOf="@id/img1"/>

    <TextView
        android:id="@+id/tx22"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="22"
        android:gravity="bottom"
        android:layout_marginStart="65dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@id/tx20k"
        app:layout_constraintTop_toTopOf="@id/img1"/>
    <TextView
        android:id="@+id/flwg"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:text="Following"
        android:gravity="bottom"
        android:layout_marginStart="15dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@id/flw"
        app:layout_constraintTop_toTopOf="@id/img1"/>

    <TextView
        android:id="@+id/user"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="Username"
        android:gravity="bottom"
        android:layout_marginStart="20dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/img1"/>

    <TextView
        android:id="@+id/adr"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="Adress"
        android:gravity="bottom"
        android:layout_marginStart="20dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/user"/>

    <TextView
        android:id="@+id/desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Description"
        android:gravity="bottom"
        android:layout_marginStart="20dp"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintTop_toBottomOf="@id/adr"/>

    <ImageView
        android:id="@+id/img2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#a9a9a9"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/desc"
        app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
        android:id="@+id/img3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#a9a9a9"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/desc"
        app:layout_constraintLeft_toRightOf="@id/img2"/>
    <ImageView
        android:id="@+id/img4"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#a9a9a9"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/desc"
        app:layout_constraintLeft_toRightOf="@id/img3"/>
    <ImageView
        android:id="@+id/img5"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#a9a9a9"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/desc"
        app:layout_constraintLeft_toRightOf="@id/img4"/>
    <ImageView
        android:id="@+id/img6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#a9a9a9"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/desc"
        app:layout_constraintLeft_toRightOf="@id/img5"/>
    <ImageView
        android:id="@+id/img7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="#a9a9a9"
        android:layout_marginStart="15dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@id/desc"
        app:layout_constraintLeft_toRightOf="@id/img6"/>

    <ImageView
        android:id="@+id/dir"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_baseline_directions_24"
        android:layout_marginTop="50dp"
        android:layout_marginStart="45dp"
        app:layout_constraintTop_toBottomOf="@id/img2"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/tx_dir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DIRECTIONS"
        android:textSize="18sp"
        android:layout_marginStart="12dp"
        android:textColor="#0085FF"
        app:layout_constraintTop_toBottomOf="@id/dir"
        app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
        android:id="@+id/call"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_baseline_phone_24"
        android:layout_marginTop="50dp"
        android:layout_marginStart="40dp"
        app:layout_constraintTop_toBottomOf="@id/img2"
        app:layout_constraintLeft_toRightOf="@id/tx_dir"/>
    <TextView
        android:id="@+id/tx_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CALL"
        android:textSize="18sp"
        android:layout_marginStart="35dp"
        android:textColor="#0085FF"
        app:layout_constraintTop_toBottomOf="@id/call"
        app:layout_constraintLeft_toRightOf="@id/tx_dir"/>
    <ImageView
        android:id="@+id/star"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_baseline_star_24"
        android:layout_marginTop="50dp"
        android:layout_marginStart="60dp"
        app:layout_constraintTop_toBottomOf="@id/img2"
        app:layout_constraintLeft_toRightOf="@id/tx_call"/>
    <TextView
        android:id="@+id/tx_str"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SAVE"
        android:textSize="18sp"
        android:layout_marginStart="55dp"
        android:textColor="#0085FF"
        app:layout_constraintTop_toBottomOf="@id/star"
        app:layout_constraintLeft_toRightOf="@id/tx_call"/>
    <ImageView
        android:id="@+id/web"
        android:layout_width="30dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_baseline_link_24"
        android:layout_marginTop="50dp"
        android:layout_marginStart="60dp"
        app:layout_constraintTop_toBottomOf="@id/img2"
        app:layout_constraintLeft_toRightOf="@id/tx_str"/>
    <TextView
        android:id="@+id/tx_web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WEBSITE"
        android:textSize="18sp"
        android:layout_marginStart="35dp"
        android:textColor="#0085FF"
        app:layout_constraintTop_toBottomOf="@id/star"
        app:layout_constraintLeft_toRightOf="@id/tx_str"/>

    <ImageView
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#a9a9a9"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/tx_dir"/>

    <ImageView
        android:id="@+id/img8"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:background="#a9a9a9"
        android:layout_marginStart="20dp"
        android:layout_marginTop="55dp"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
        android:id="@+id/img9"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:background="#a9a9a9"
        android:layout_marginStart="20dp"
        android:layout_marginTop="55dp"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintLeft_toRightOf="@id/img8"/>
    <ImageView
        android:id="@+id/img10"
        android:layout_width="110dp"
        android:layout_height="110dp"
        android:background="#a9a9a9"
        android:layout_marginStart="20dp"
        android:layout_marginTop="55dp"
        app:layout_constraintTop_toBottomOf="@id/line"
        app:layout_constraintLeft_toRightOf="@id/img9"/>

</androidx.constraintlayout.widget.ConstraintLayout>